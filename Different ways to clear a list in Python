arr = [10,20,4,4646,5346,564]
arr.clear()
print(arr)
------------------ OR --------------
arr = [10,20,4,4646,5346,564]
arr = []
print(arr)
------------------ OR ----------------
arr = [10,20,4,4646,5346,564]
arr *= 0
print(arr)
------------------- OR -----------------
arr = [10,20,4,4646,5346,564]
del arr[:]
print(arr)
