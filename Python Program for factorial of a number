def facto(n):
    fact = 1
    if n==0 or n==1:
        return 1
    else:
        for i in range(1,n+1):
            fact = fact * i
        return fact
n=int(input("Enter factorial number: "))
facto(n)


-----------------OR-----------------
def facto(n):
    fact = 1
    if n==0 or n==1:
        return 1
    else:
        fact = n * facto(n-1)
    return fact
n=int(input("Enter factorial number: "))
facto(n)
